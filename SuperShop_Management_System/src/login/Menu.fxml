            <VBox prefHeight="798.0" prefWidth="199.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="p1" mnemonicParsing="false" onMouseClicked="#Product" prefHeight="59.0" prefWidth="142.0" text="Product">
                     <VBox.margin>
                        <Insets left="20.0" top="70.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="p2" layoutX="20.0" layoutY="90.0" mnemonicParsing="false" onMouseClicked="#Supply" prefHeight="59.0" prefWidth="142.0" text="Supply">
