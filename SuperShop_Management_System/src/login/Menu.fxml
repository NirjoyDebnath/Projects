                  </Button>
                  <Button fx:id="p3" layoutX="20.0" layoutY="90.0" mnemonicParsing="false" onMouseClicked="#Stock" prefHeight="59.0" prefWidth="142.0" text="Stock">
                     <VBox.margin>
                        <Insets left="20.0" top="70.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="p4" mnemonicParsing="false" onMouseClicked="#Transaction" prefHeight="59.0" prefWidth="140.0" text="Transaction">
                     <VBox.margin>
                        <Insets left="20.0" top="70.0" />
                     </VBox.margin></Button>
                  <Button fx:id="p5" mnemonicParsing="false" onMouseClicked="#Addmember" prefHeight="58.0" prefWidth="140.0" text="Add member">
                     <VBox.margin>
                        <Insets left="20.0" top="70.0" />
                     </VBox.margin></Button>
